<header>
    <div class="header-container" >
      <div class="header-part">
        <a href="/">
          <div class="header-home-container"> 
            <img class="home-icon" src="../icons/home.png"> 
            <p>Home</p>
          </div>
        </a>

        <div class="header-home-container" style="width: 15px;">
            <img style="width: 100%; padding-left: 5px; padding-right: 5px;" src="/icons/shark.png" >
        </div>
      </div>

      

      <div class="header-part">
        <a href="/sort?type=<%= articleData.cathegory.id %>">
          <div class="header-home-container"> 
            <p><%= articleData.cathegory.name %></p>
          </div>
        </a>
        <div class="header-home-container" style="width: 15px;">
            <img style="width: 100%; padding-left: 5px; padding-right: 5px;" src="/icons/shark.png" >
        </div>
      </div>

      

      <div class="header-part">
        <a href="/article?id=<%= articleData.articleId %>">
          <div class="header-home-container"> 
            <p><%= articleData.title %></p>
          </div>
        </a>
      </div>         

    </div>
